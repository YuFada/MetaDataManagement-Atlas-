  <!--查询不包含集合中的字段名称-->
  <select id="getFieldByTableNameAndUse" resultType="java.util.Map">
    select COLUMN_NAME name,COLUMN_TYPE type,COLUMN_COMMENT fieldDescribe,table_name tableName,TABLE_SCHEMA databaseName from information_schema.columns WHERE 1=1
    <if test="database != null and database != ''">
