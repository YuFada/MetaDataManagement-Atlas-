    <script  th:src="@{/systemsite/assets/lib/layui/layui.js}"></script>
    <script type="text/javascript">
        
        	layui.extend({
				admin: '{/}../../../systemsite/assets/static/js/admin'
			});
            layui.use(['form','admin'], function(){
              var form = layui.form
              	,admin = layui.admin;
              //监听提交
              form.on('submit(login)', function(data){
                console.log(data.field);
                  var params=data.field;
                  params['type']=4;
                  system.post("/system/user/login",params,function(data){
                      console.log(data)
                      if(data.code=="200"){
                          window.location.href=data.result.url;
                      }else{
                          system.tip(data.msg,"0");
                      }
                  })
                return false;
              });
            });

            $(document).keypress(function(e) {
               $(".layui-card").click(function(){
                   $(this).addClass("layui-anim")
               })
            });
    </script>  
